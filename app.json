{
  "expo": {
    "name": "MovieExplorer",
    "slug": "MovieExplorer",
    "version": "1.0.0",
    "newArchEnabled": true,
    "icon": "./assets/images/android-icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181c"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/android-icon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "507f9eaf-cd83-4e7d-bacd-b823593fd25f"
      }
    },
    "android": {
      "package": "com.rollinsoul.MovieExplorer",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#18181c"
      }
    },
    "ios": {
      "bundleIdentifier": "com.rollinsoul.MovieExplorer",
      "supportsTablet": true,
      "icon": "./assets/images/ios-icon.png"
    }
  }
}
